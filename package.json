{
  "name": "prisma-schema-transformer",
  "description": "Transform names from snake_case to camelCase in Prisma schema.",
  "version": "0.2.1",
  "repository": "https://github.com/IBM/prisma-schema-transformer",
  "author": {
    "name": "Michael Lin",
    "email": "hi@michaellin.me",
    "url": "https://michaellin.me"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "prisma-schema-transformer": "bin.js"
  },
  "files": [
    "dist",
    "bin.js"
  ],
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": ".build/"
      }
    },
    "require": [
      "ts-node/register"
    ]
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tscconfig.json",
    "test": "ava",
    "lint": "xo --fix"
  },
  "dependencies": {
    "@prisma/cli": "^2.1.3",
    "@prisma/generator-helper": "^2.1.3",
    "@prisma/sdk": "^2.1.3",
    "arg": "^4.1.3",
    "camelcase": "^6.0.0",
    "immer": "^6.0.6",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/pluralize": "^0.0.29",
    "ava": "^3.8.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.5",
    "xo": "^0.32.0"
  }
}
